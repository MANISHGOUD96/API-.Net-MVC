@model  List<MK_API_WebMVC.Models.EmpModel>

@{
    ViewBag.Title = "Home Page";
}

<h1>Emp Table</h1>

<table class="table table-bordered table-hover table-responsive">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Age</th>
        <th>Adress</th>
        <th>Company</th>
        <th>Salary</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Addhar</th>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.Id</td>
    <td>@item.Name</td>
    <td>@item.Age</td>
    <td>@item.Adress</td>
    <td>@item.Company</td>
    <td>@item.Salary</td>
    <td>@item.Email</td>
    <td>@item.Phone</td>
    <td>@item.Adress</td>
    <td><a class="btn btn-primary" href="@Url.Action("Edit","Home",new {item.Id})">Edit</a></td>
    <td><a class="btn btn-danger" href="@Url.Action("Delete","Home",new {item.Id})">Delete</a></td>
</tr>

    }

</table>